{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@components/*": ["components/*"],
      "@context/*": ["context/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@customTypes/*": ["types/*"],
      "@styles/*": ["styles/*"],
      "@pages/*": ["pages/*"]
    }
  }
}
